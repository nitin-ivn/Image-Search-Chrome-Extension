{
    "manifest_version": 3,
    "name": "Image Search",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "/popup/popup.html"
    },
    "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}